@{
    ViewBag.Title = "LogOff";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section css {
    @Styles.Render("~/Content/narrowjumbo")
}
<div class="container">
    <!-- Static navbar -->
    <div class="navbar navbar-default" role="navigation">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span> <span class="icon-bar"></span><span
                    class="icon-bar"></span><span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="@Url.Action("index", new { Controller = "home" })">myLegis</a>
        </div>
        <div class="navbar-collapse collapse">
            @Html.Partial("_navbar-primary-navigation")
            <ul class="nav navbar-nav navbar-right">
                <li><a href="@Url.Action("Index", new { Controller = "acct" })">myAccount</a></li>
            </ul>
        </div>
        <!--/.nav-collapse -->
    </div>
    <ol class="breadcrumb">
        <li><a href="@Url.Action("index", new { Controller = "home" })">Home</a> </li>
        <li class="active">OpenID Log Off</li>
    </ol>
    <div class="row marketing">
        <div class="col-lg-12">
            <h4>
                Logged Out</h4>
            <p>
                We hate to see you go, but hey, it happens.</p>
            <p>
                You've been logged off of the myLegis web application.</p>
        </div>
    </div>
    <div class="footer">
        @Html.Partial("_footer")
    </div>
</div>
<!-- /container -->
